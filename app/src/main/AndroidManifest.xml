<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.eshopbd">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".products_to_beDisplayed.construction_products_contain_recycle.Block_all"></activity>
        <activity android:name=".products_to_beDisplayed.construction_products_contain_recycle.Stone_all" />
        <activity android:name=".products_to_beDisplayed.construction_products_contain_recycle.Rod_all" />
        <activity android:name=".products_to_beDisplayed.construction_products_contain_recycle.Sand_all" />
        <activity android:name=".products_to_beDisplayed.construction_products_contain_recycle.Cement_all" />
        <activity android:name=".products_to_beDisplayed.construction_products_contain_recycle.Brick_all" />
        <activity android:name=".Construction_materials" />
        <activity android:name=".ClosingActivity" />
        <activity android:name=".Account" />
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:configChanges="orientation|screenSize|keyboardHidden"
            android:label="@string/app_name"
            android:theme="@style/AppTheme" />
    </application>

</manifest>